<block wx:if="{{url}}"><view class="audio"><view style="width:100%;"><slider block-size="6" block-color="black" activeColor="{{activeColor}}" min="{{0}}" max="{{$root.g0}}" value="{{$root.g1}}" step="{{0.1}}" data-event-opts="{{[['change',[['changeAudio',['$event']]]]]}}" bindchange="__e"></slider></view><view style="width:100%;display:flex;flex-direction:row;"><view style="width:20%;display:flex;justify-content:center;">{{''+elapsedTime+''}}</view><view style="width:60%;"></view><view style="width:20%;display:flex;align-items:center;justify-content:center;">{{'-'+remainingTime+''}}</view></view><view style="width:100%;display:flex;align-items:center;justify-content:center;flex-direction:row;margin-top:6%;"><image style="margin-right:5%;width:56rpx;height:56rpx;" src="../../static/temp_pic/backward-15.png" data-event-opts="{{[['tap',[['seekBackward',['$event']]]]]}}" bindtap="__e"></image><image hidden="{{!(!status)}}" class="icon" src="{{startPic}}" data-event-opts="{{[['tap',[['start',['$0'],['audioId']]]]]}}" bindtap="__e"></image><image hidden="{{!(status)}}" class="icon" src="{{endPic}}" data-event-opts="{{[['tap',[['start',['$0'],['audioId']]]]]}}" bindtap="__e"></image><image style="margin-left:5%;width:58rpx;height:56rpx;" src="../../static/temp_pic/forward-15.png" data-event-opts="{{[['tap',[['seekForward',['$event']]]]]}}" bindtap="__e"></image></view></view></block>