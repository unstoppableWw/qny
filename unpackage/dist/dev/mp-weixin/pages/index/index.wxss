.bowen{
	position: absolute;
	width: 100%;
	height: 40%;
	background-image: url(/static/common/background_4.png);
	background-size: cover;
	z-index:-1;
}
.index_top{
	display: flex;
	flex-direction: column;
	justify-content: space-evenly;
	align-items: center;
	background-size: cover;
	height: 32%;
	margin-top: 60rpx;
}
.swiper-item-index{
	display: flex;
	justify-content: center;
	align-items: center;
	height: 300rpx;
	width: 90%;
}
.uni-swiper-dots  {
	margin-bottom: 20rpx;
}
.index_bottom{
	
	
	display: flex;
	align-items: flex-center;
	justify-content: flex-end;
	flex-wrap: wrap;
	height: 63%;
}
.index_con{
	display: flex;
	
	padding-top: 10rpx;
	align-items: center;
	justify-content: center;
	width: 550rpx;
}
.index_anniu{
	display: flex;
	flex: 1;
	color: #4095E5;
	justify-content: center;
	align-items: center;
	
	flex-direction:column
}
.index_anniu:hover{
	-webkit-animation: jello;
	        animation: jello;
	-webkit-animation-duration: 1s;
	        animation-duration: 1s;
}
.pipei_image:hover{
	-webkit-animation:pulse;
	        animation:pulse;
	-webkit-animation-duration:1s;
	        animation-duration:1s;
}
.shaixuan:hover{
	-webkit-animation:jello;
	        animation:jello;
	-webkit-animation-duration:1s;
	        animation-duration:1s;
}
/* 以下是弹框的样式 */
.popup_overlay {
	position: fixed;
	top: 0%;
	left: 0%;
	width: 100%;
	height: 100%;
	background-color: black;
	z-index: 1001;
	opacity: .80;
}
.popup_content {
	position: fixed;
	top: 20%;
	left: 10%;
	width: 75%;
	height: 800rpx;
	border-radius: 30rpx;
	border: 10px solid white;
	background-color: white;
	z-index: 1002;
}
.popup_content_jinfang {
	position: fixed;
	top: 20%;
	left: 10%;
	width: 75%;
	height: 400rpx;
	border-radius: 30rpx;
