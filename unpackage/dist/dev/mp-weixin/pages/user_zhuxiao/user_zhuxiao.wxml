<view class="head_bg"><view class="flex flex-column align-center justify-center" style="height:30%;"><view style="font-size:50rpx;font-weight:700;">注销账号</view></view><view class="chushi_shuru"><image class="small_icon" src="../../static/head/shoujihaoma.svg"></image><view style="width:5rpx;height:90rpx;background-color:#DDDDDD;margin-right:40rpx;"></view><input style="font-size:30rpx;" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','user_phone','$event',[]]]]]]}}" value="{{user_phone}}" bindinput="__e"/></view><view class="chushi_shuru"><image class="small_icon" src="../../static/head/yanzhengma.svg"></image><view style="width:5rpx;height:90rpx;background-color:#DDDDDD;margin-right:40rpx;"></view><input hidden="{{!(login_manner_is)}}" style="font-size:30rpx;" placeholder="{{login_manner}}" data-event-opts="{{[['input',[['__set_model',['','yanzheng_code','$event',[]]]]]]}}" value="{{yanzheng_code}}" bindinput="__e"/><view hidden="{{!(login_manner_is)}}" class="huoqu_yanzheng flex justify-center align-center" style="color:white;"><block wx:if="{{is_yanzheng}}"><view data-event-opts="{{[['tap',[['huoqu_yanzheng',['$event']]]]]}}" style="width:80%;" bindtap="__e">获取验证</view></block><block wx:else><text>{{yanzheng_time}}</text></block></view><input hidden="{{!(!login_manner_is)}}" style="font-size:30rpx;" placeholder="{{login_manner}}" password="{{true}}" type="text" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><button class="chushi_denglu" style="color:white;font-size:30rpx;margin-top:5%;" loading="{{loading}}" data-event-opts="{{[['tap',[['denglu',['$event']]]]]}}" bindtap="__e">{{this.loading?'验证中...':'注销'}}</button></view>